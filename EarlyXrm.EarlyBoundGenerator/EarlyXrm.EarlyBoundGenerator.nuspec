<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata minClientVersion="2.5">
    <id>EarlyXrm.EarlyBoundGenerator</id>
    <version>0.0.1</version>
    <authors>a berman</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
	<license type="expression">MIT</license>
	<licenseUrl>https://licenses.nuget.org/MIT</licenseUrl>
    <description>Early bound generator based on svcutil.exe</description>
	<repository url="https://github.com/bermo/EarlyXrm.EarlyBoundGenerator" />
    <dependencies>
      <group targetFramework=".NETFramework4.6.2">
        <dependency id="Microsoft.CrmSdk.CoreAssemblies" version="9.1.0.77" exclude="Build,Analyzers" />
	  </group>
    </dependencies>
    <contentFiles />
  </metadata>
  <files>
	  
	<file src="EarlyXrm.EarlyBoundGenerator.targets" target="build\EarlyXrm.EarlyBoundGenerator.targets" />

	<file src="bin\$configuration$\EarlyXrm.EarlyBoundGenerator.exe" target="build\EarlyXrm.EarlyBoundGenerator.exe" />
	<file src="bin\$configuration$\CrmSvcUtil.exe" target="build\CrmSvcUtil.exe" />
	<file src="bin\$configuration$\Microsoft.Crm.Sdk.Proxy.dll" target="build\Microsoft.Crm.Sdk.Proxy.dll" />
	<file src="bin\$configuration$\Microsoft.Xrm.Sdk.dll" target="build\Microsoft.Xrm.Sdk.dll" />
	<file src="bin\$configuration$\Microsoft.Xrm.Tooling.CrmConnectControl.dll" target="build\Microsoft.Xrm.Tooling.CrmConnectControl.dll" />
	<file src="bin\$configuration$\Microsoft.Xrm.Tooling.Connector.dll" target="build\Microsoft.Xrm.Tooling.Connector.dll" />
	<file src="bin\$configuration$\Microsoft.IdentityModel.Clients.ActiveDirectory.dll" target="build\Microsoft.IdentityModel.Clients.ActiveDirectory.dll" />
	<file src="bin\$configuration$\Newtonsoft.Json.dll" target="build\Newtonsoft.Json.dll" />
	
	<file src="generate.bat" target="build\generate.bat" />
	
	<file src="earlybound.bat" target="build\earlybound.bat" />
	<file src="earlybound.json" target="build\earlybound.json" />
	  
	<file src="earlybound.bat" target="content\bin\EarlyXrm\earlybound.bat" />
	<file src="earlybound.json" target="content\bin\EarlyXrm\earlybound.json" />
	  
  </files>
</package>